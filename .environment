#!/bin/sh
PATH="$HOME/.local/bin:/usr/lib/ccache:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games"
LC_COLLATE=C
EDITOR="$HOME/.local/bin/vim-wrapper"
PAGER=less
LESS='-iR'
LESSHISTFILE=-
GZIP='-9'
ENSCRIPT='--header=|$n --highlight --media=letter --color'
GREP_OPTIONS='--color=auto'
GREP_COLOR='1;32'
export PATH LC_COLLATE EDITOR LESS LESSHISTFILE GZIP ENSCRIPT GREP_OPTIONS GREP_COLOR

EMAIL='josh@freedesktop.org'
NAME='Josh Triplett'
DEBEMAIL=$EMAIL
DEBFULLNAME="$NAME"
GIT_AUTHOR_NAME="$NAME"
GIT_AUTHOR_EMAIL=$EMAIL
GIT_COMMITTER_NAME="$NAME"
GIT_COMMITTER_EMAIL=$EMAIL
export EMAIL NAME DEBEMAIL DEBFULLNAME GIT_AUTHOR_NAME GIT_AUTHOR_EMAIL GIT_COMMITTER_NAME GIT_COMMITTER_EMAIL
